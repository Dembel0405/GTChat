{
    "env": {
        "node": true,
        "es2021": true,
        "browser": true
    },
    "extends": [
        "eslint:recommended",
        "prettier"
    ],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 2021
    },
    "rules": {
        "prettier/prettier": "error",
        "no-console": "warn",
        "no-unused-vars": "warn",
        "prefer-const": "error",
        "no-var": "error"
    }
}
